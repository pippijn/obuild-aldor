OM_CFLAGS	+= $(STRICT_CFLAGS)
OM_CPPFLAGS	+= -DTEST_ALL=1 -I$(dir .)

SHARED = false
STATIC = true

$(pkg-config-name $(Name)): $(install-target $(includedir)/aldor, $(C-Headers))

compute-recurse-subdirs ($(C-Sources))
TARGET = $(library-install)
.DEFAULT: $(TARGET)
