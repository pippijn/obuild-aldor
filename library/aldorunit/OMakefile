Name = aldorunit
Description = Aldor unit testing
Version = 1.0.3

Aldor-Sources[] =
  src/exceptions/tst_afe
  src/exceptions/tst_afet
  src/framework/tst_tct
  src/framework/tst_tctl
  src/framework/tst_tstl
  src/version/tst_vers

Interfaces = include/aldorunit

Aldor-Requires[] =
  aldor
  algebra
  extio

OM_AFLAGS[] =
  -DLibraryAldorUnitRequiresLibraryExtIO
  -DLibraryAldorUnitRequiresLibraryAlgebra

TARGET = $(library-install)
.DEFAULT: $(TARGET)
